<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.example.iouapp">
    <uses-permission android:name="android.permission.INTERNET" />
    <application android:label="IOU App" android:hasCode="true">
        <activity android:name="android.app.NativeActivity" android:label="IOU App" android:exported="true">
            <meta-data android:name="android.app.lib_name" android:value="iou_app" />
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
